<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="edu.eci.cvds.persistence.mybatis.mappers.EquipoMapper">
    <select id="consultarEquipos" parameterType="map" resultMap="EquipoResult" >
    SELECT
        idEquipo AS eq_idEquipo,
        laboratorio AS eq_laboratorio,
        fechaRegistro AS eq_fecha,
        idTorre AS eq_torre,
        idPantalla AS eq_pantalla,
        idMouse AS eq_mouse,
        idTeclado AS eq_teclado,
        disponible AS eq_disponible
    FROM Equipo;
    </select>
    <resultMap id="EquipoResult" type="Equipo">
        <id property="idEquipo" column="eq_idEquipo"/>
        <result property="laboratorio" column="eq_laboratorio"/>
        <result property="fechaRegistro" column="eq_fecharegistro"/>
        <result property="idTorre" column="eq_torre"/>
        <result property="idPantalla" column="eq_pantalla"/>
        <result property="idMouse" column="eq_mouse"/>
        <result property="idTeclado" column="eq_teclado"/>
        <result property="disponible" column="eq_disponible"/>
    </resultMap>
</mapper>